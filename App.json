      "package": "com.shefferjason.sportslogicai",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shefferjason.sportslogicai"
    }
  }
}