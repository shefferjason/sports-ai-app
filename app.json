{
  "expo": {
    "name": "SportsLogicAI",
    "slug": "sportslogicai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "sportslogicai",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shefferjason.sportslogicai"
    },

    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },

    "plugins": [],

    "extra": {
      "eas": {
        "projectId": "85d11519-e953-4076-8165-d4cb55e6dc19"
      }
    }
  }
}